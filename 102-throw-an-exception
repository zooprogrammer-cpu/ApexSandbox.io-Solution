public List<Account> checkAccounts(List<Account> accounts)
{
    // code here 

    // if (accounts.size() == null){
    //     throw new IllegalArguemntException('accounts should not be null');
    // }

    for (Account acc : accounts){
        if (acc.BillingCity != null){
            Boolean returnList = true;  
        }
        
    }

    if (returnList == true){
        return accounts; 
    }

    for (Account acc : accounts){
        if (acc.BillingCity == null){
            throw new AccountException();
        }
    }
}

//do not remove the following custom-defined exception
public class AccountException extends Exception {}