public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
    //code here
    Set<Integer> set1 = new Set<Integer>(); 
    Set<Integer> set2 = new Set<Integer>(); 

    set1.addAll(nums1);
    set1.sort(); 
    set2.addAll(nums2); 
    set2.sort(); 

    for (Integer set1Elem : set1){
        for (Integer set2Elem : set2){
            if (set1Elem == set2Elem){
                return true; 
            }
            else return false; 
        }
    }



}