public List<apxio__Student__c> selectUnreachableStudents() {
    // code here
    List <apxio__Student__c> studList = [SELECT Id, Name , apxio__Active__c, apxio__Email__c,apxio__Phone__c  FROM apxio__Student__c 
    WHERE apxio__Active__c = true AND apxio__Email__c = null AND apxio__Phone__c = null ORDER BY Name];
    return studList;
}