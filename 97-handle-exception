public String divide(Integer a, Integer b){
    //code here
    Decimal aDec = decimal.valueOf(a);
    Decimal bDec = decimal.valueOf(b);

    try {
        Decimal dividedVal = aDec/bDec;
        return String.valueOf(dividedVal) ; 
        
    }
    catch (Exception e){
        System.debug(e.getMessage()); 
    }
   
}