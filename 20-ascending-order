public Boolean ascendingOrder(Integer a, Integer b, Integer c) {
    //code here
    List<Integer> integerList = new <List> ( a, b, c);
    integerList.sort(); 
    List<Integer> sortedList = integerList; 
    if (integerList == sortedList){
        return true; 
    } else{
        return false; 
    }


       



}