public List<String> getFieldsValue(Account acc, List<String> fields)
{
    // code here
    //List <Account> accList = [SELECT Id FROM Account WHERE Id =: acc.Id];
    List <String> fieldOutputList = new List <String>(); 

    for (String field : fields){
        fieldOutputList.add(SObject.get('field')]) ;
    }
    return fieldOutputList; 
}