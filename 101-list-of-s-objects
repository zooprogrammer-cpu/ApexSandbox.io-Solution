public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here
    List<SObject> list = new List <SObject>(); 
    for (Integer i = 0; i < accounts.size(); i++) {
        list.add(accounts[0]);
        list.add(contacts[0]);
        
    }
    return list; 
}