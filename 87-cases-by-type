public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map <String, List<Case>> newMap = new Map <String, List<Case>>();

    for (Case cs: cases){
        if (cs.Type != null){
            if (newMap.containsKey(cs.Type)){
                newMap.get(cs.Type).add(cs); 
            }
            else{
                newMap.put(c.Type, new List<Case>{cs});
            }
        }
    }
    return newMap;
}