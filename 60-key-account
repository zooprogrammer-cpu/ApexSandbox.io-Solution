public Boolean isKeyAccount(Account a) {
    //code here
    if (a.Industry == 'Banking' && a.AnnualRevenue > 600000){
        return true; 
    }
    else {
        return false; 
    }

    if (a.Industry == 'Technology' && a.AnnualRevenue > 800000){
        return true;
    }
    else {
        return false;
    }
}